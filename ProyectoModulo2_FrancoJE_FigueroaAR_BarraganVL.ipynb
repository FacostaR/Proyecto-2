    "def amortizacion_tabla(capital, tasa, años,\n",
    "                       enganche=0, pagos_año=12, fecha_inicio=date.today()):\n",
    "\n",
    "    # El pago se mantiene constante según los términos originales del préstamo.\n",
    "    pago = -round(np.pmt(tasa/pagos_año, años*pagos_año, capital), 2)\n",
    "    \n",
    "    # Genera la tabla y ordena las columnas resultantes para su conveniencia.\n",
    "    tabla = pd.DataFrame(amortizacion(capital, tasa, años, pago,\n",
    "                                     enganche, fecha_inicio, pagos_año))\n",
    "    tabla = tabla[[\"Periodo\", \"Mes\", \"Balance inicial\", \"Pago\", \"Interes\", \n",
    "                         \"Capital\", \"Enganche\", \"Balance final\"]]\n",
    "    \n",
    "    # Convertir a un objeto de fecha\n",
    "    tabla[\"Mes\"] = pd.to_datetime(tabla[\"Mes\"])\n",
    "    \n",
    "    #Tabla de estadística de resumen\n",
    "    fecha_final = tabla[\"Mes\"].iloc[-1]\n",
    "    res = pd.Series([fecha_final, tabla[\"Periodo\"].count(), tasa,\n",
    "                       años, capital, pago, enganche,\n",
    "                       tabla[\"Interes\"].sum()],\n",
    "                       index=[\"Fecha de termino\", \"Número de pagos\", \"Tasa de interes\", \"Años\", \"Capital\",\n",
    "                             \"Pago\", \"Enganche\", \"Interes total\"])\n",
    "    \n",
    "    return tabla, res"
   ]
