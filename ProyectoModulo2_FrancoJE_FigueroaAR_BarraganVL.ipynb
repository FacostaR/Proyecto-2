  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def amortizacion(capital, tasa, años, pmt, enganche, fecha_inicio, pagos_año):\n",
    "\n",
    "    # inicializa las variables\n",
    "    p = 1\n",
    "    ini_balance = capital\n",
    "    fin_balance = capital\n",
    "    \n",
    "    while fin_balance > 0:\n",
    "        \n",
    "        # Recalcular el interés basado en el balance \n",
    "        interes = round(((tasa/pagos_año) * ini_balance), 2)\n",
    "        \n",
    "        # Determine el pago según si este período pagará o no el préstamo\n",
    "        pmt = min(pmt, ini_balance + interes)\n",
    "        capital = pmt - interes\n",
    "        \n",
    "        # Asegúrese de que el pago adicional se ajuste si el préstamo se está pagando\n",
    "        enganche = min(enganche, ini_balance - capital)\n",
    "        fin_balance = ini_balance - (capital + enganche)\n",
    "\n",
    "        yield OrderedDict([('Mes',fecha_inicio),\n",
    "                           ('Periodo', p),\n",
    "                           ('Balance inicial', ini_balance),\n",
    "                           ('Pago', pmt),\n",
    "                           ('Capital', capital),\n",
    "                           ('Interes', interes),\n",
    "                           ('Enganche', enganche),\n",
    "                           ('Balance final', fin_balance)])\n",
    "        \n",
    "        # Incrementar el contador, saldo y fecha.\n",
    "        p += 1\n",
    "        fecha_inicio += relativedelta(months=1)\n",
    "        ini_balance = fin_balance"
   ]
  },
